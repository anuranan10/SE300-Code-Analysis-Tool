<Window x:Class="CodeAnalysisTool.ResultsScreen.ResultsScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="CAT" Height="550" Width="700" WindowStartupLocation="CenterScreen">

    <Grid Background="#f9f9f9">
        <!-- Title -->
        <TextBlock Text="Code Analytics" HorizontalAlignment="Center" VerticalAlignment="Top"
                   FontSize="26" FontWeight="Bold" Margin="0,40,0,0" />

        <!-- Return Button -->  
        <Button Content="Return to Dashboard" Width="210" Height="45" 
        HorizontalAlignment="Center" VerticalAlignment="Top" 
        Margin="0,450,0,0" Background="Green" Foreground="Black" 
        FontSize="20" FontWeight="DemiBold"
        Click="ReturnToDashboard_Click" />
            
    </Grid>
</Window>